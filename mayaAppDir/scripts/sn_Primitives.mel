//	 sn_Primitives
//	
//	Author: Sami Nikki
//	Contact: saminikki@hotmail.com
//	
//	This script creates primitives ala 3dsMax. 
//	I will create more objects as I need them but
// 	you're free to modify this script as you see fit. 
//	
//	Installation:
//	The icon images go in the aw/icons directory. 
//	The script wherever your script path points to ex. /admin/scripts
//
//	If you have any suggestions please email me and I'll update the script. 


global proc sn_Primitives ( )
{

windowPref -ra;
string $sami_window = `window -widthHeight 210 400 -title "sn_Primitives" -iconName "sn_Primitives"`;

string $form = `formLayout`;

string $a1 = `iconTextButton -st "iconOnly" -i1 "roundbox.bmp" -w 210 -h 50 -vis 1 
-command sn_createroundbox`; 

string $b1 = `iconTextButton -st "iconOnly" -i1 "sharpbox.bmp" -w 210 -h 50 -vis 1
-command sn_createsharpbox `;	

string $c1 = `iconTextButton -st "iconOnly" -i1 "roundbowl.bmp" -w 210 -h 50 -vis 1 
-command sn_createroundbowl2`;

string $d1 = `iconTextButton -st "iconOnly" -i1 "sharpbowl.bmp" -w 210 -h 50 -vis 1 
-command sn_createsharpbowl`;

string $e1 = `iconTextButton -st "iconOnly" -i1 "solidtube.bmp" -w 210 -h 50 -vis 1 
-command sn_createsolidtube`;

string $f1 = `iconTextButton -st "iconOnly" -i1 "roundcylinder.bmp" -w 210 -h 50 -vis 1 
-command sn_createroundcylinder`;

string $g1 = `iconTextButton -st "iconOnly" -i1 "roundcone.bmp" -w 210 -h 50 -vis 1 
-command sn_createroundcone`;


string $s1 = `iconTextButton -st "iconOnly" -i1 "credit.bmp" -w 210 -h 25 -vis 1
-command ""`;


formLayout -edit
-attachForm     $a1    "top"    0
-attachForm     $a1     "left"  0

-attachControl  $b1    "top"   0 $a1
-attachForm  $b1     "left" 0 

-attachControl  $c1    "top"   0 $b1
-attachForm  $c1     "left" 0 

-attachControl  $d1    "top"   0 $c1
-attachForm  $d1     "left" 0 

-attachControl  $e1    "top"   0 $d1
-attachForm  $e1     "left" 0 

-attachControl  $f1    "top"   0 $e1
-attachForm  $f1     "left" 0 

-attachControl  $g1    "top"   0 $f1
-attachForm  $g1     "left" 0 

-attachControl  $s1    "top"   0 $g1
-attachForm  $s1     "left" 0 

$form;

setParent ..;
showWindow $sami_window;
}




