global proc growSelection (){int $convertFrom;if (whatIsSel() == "v")	$convertFrom = 3;if (whatIsSel() == "e")	$convertFrom = 2;if (whatIsSel() == "f")	$convertFrom = 1;int $convertHelp;if ($convertFrom == 3)	$convertHelp = 1;if ($convertFrom == 2)	$convertHelp = 3;if ($convertFrom == 1)	$convertHelp = 3;eval ("PolySelectConvert " + $convertHelp);eval ("PolySelectConvert " + $convertFrom);}